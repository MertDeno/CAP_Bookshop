{
    "_version": "1.45.0",
    "sap.app": {
        "id": "bookshop.cap.bookshop",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:lrop",
            "version": "1.8.1",
            "toolsId": "84d98df8-dffc-4f45-bab8-ba8865c34315"
        },
        "dataSources": {
            "mainService": {
                "uri": "/RegisterService/",
                "type": "OData",
                "settings": {
                    "annotations": [
                        "annotation"
                    ],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "4.0"
                }
            },
            "annotation": {
                "type": "ODataAnnotation",
                "uri": "annotations/annotation.xml",
                "settings": {
                    "localUri": "annotations/annotation.xml"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.111.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ushell": {},
                "sap.fe.templates": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "bookshop.cap.bookshop.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        },
        "resources": {
            "css": []
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "NationsList",
                    "target": "NationsList"
                },
                {
                    "pattern": "Nations({key}):?query:",
                    "name": "NationsObjectPage",
                    "target": "NationsObjectPage"
                },
                {
                    "pattern": "Nations({key})/authors({key2}):?query:",
                    "name": "AuthorsObjectPage",
                    "target": "AuthorsObjectPage"
                }
            ],
            "targets": {
                "NationsList": {
                    "type": "Component",
                    "id": "NationsList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "Nations",
                            "variantManagement": "Page",
                            "navigation": {
                                "Nations": {
                                    "detail": {
                                        "route": "NationsObjectPage"
                                    }
                                }
                            },
                            "controlConfiguration": {
                                "@com.sap.vocabularies.UI.v1.LineItem": {
                                    "tableSettings": {
                                        "condensedTableLayout": true,
                                        "selectAll": false,
                                        "selectionMode": "None",
                                        "quickVariantSelection": {
                                            "showCounts": true
                                        },
                                        "type": "ResponsiveTable"
                                    }
                                }
                            }
                        }
                    }
                },
                "NationsObjectPage": {
                    "type": "Component",
                    "id": "NationsObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": false,
                            "entitySet": "Nations",
                            "navigation": {
                                "authors": {
                                    "detail": {
                                        "route": "AuthorsObjectPage"
                                    }
                                }
                            },
                            "controlConfiguration": {
                                "authors/@com.sap.vocabularies.UI.v1.LineItem#Authors": {
                                    "columns": {
                                        "DataFieldForAnnotation::DataPoint::likes": {
                                            "width": "20%"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "AuthorsObjectPage": {
                    "type": "Component",
                    "id": "AuthorsObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": false,
                            "entitySet": "Authors",
                            "controlConfiguration": {
                                "books/@com.sap.vocabularies.UI.v1.LineItem#Books": {
                                    "columns": {
                                        "DataFieldForAnnotation::DataPoint::edition1": {
                                            "width": "20%"
                                        },
                                        "ViewedFragment": {
                                            "header": "Available",
                                            "position": {
                                                "placement": "After",
                                                "anchor": "DataField::genre"
                                            },
                                            "template": "bookshop.cap.bookshop.ext.fragment.ViewedFragment"
                                        }
                                    }
                                }
                            },
                            "navigation": {}
                        }
                    }
                }
            }
        },
        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {}
            }
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    }
}